{
  "Touchpoint": {
    "FacilitySettingsDataPath": "App_Data/FacilitySettings.json",
    "Connection": "valkey:6379,abortConnect=false,connectRetry=5,connectTimeout=5000,keepAlive=30",
    "KeyPrefix": "cache",
    "Enabled": false,
    "WindowMs": 5000,
    "TimeToLiveHr": 24,
    "ProcessorDelayMs": 2000
  },
  "PCC": {
    "AppName": "tpm-development",
    "AppKey": "T57xKUv7NJj4Rfm4WFfB6jel9pp8ORAy",
    "AppKeySecret": "4smA3SDjKPQqgDRv",
    "ClientCertificateDataPath": "App_Data/PointClickCare/dev.gateway.pfx",
    "ClientCertificatePassword": "TPM2025",
    "WebhookUsername": "touchpoint-medical-notifier",
    "WebhookPassword": "ABC!123abd#"
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File",
      "Serilog.Enrichers.Environment",
      "Serilog.Enrichers.Process",
      "Serilog.Enrichers.Thread",
      "Serilog.Expressions"
    ],
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "Serilog.AspNetCore.RequestLoggingMiddleware": "Information"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithEnvironmentName",
      "WithProcessId",
      "WithThreadId"
    ],
    "Properties": {
      "Application": "TouchpointMedical Integrations"
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "formatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact"
        }
      },
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "File",
              "Args": {
                "path": "Logs/app-.json",
                "rollingInterval": "Day",
                "rollOnFileSizeLimit": true,
                "fileSizeLimitBytes": 10485760,
                "retainedFileCountLimit": 7,
                "formatter": "Serilog.Formatting.Json.JsonFormatter, Serilog",
                "formatterArgs": {
                  "renderMessage": true
                }
              }
            }
          ]
        }
      },
      {
        "Name": "Logger",
        "Args": {
          "configureLogger": {
            "Filter": [
              {
                "Name": "ByIncludingOnly",
                "Args": { "expression": "StartsWith(SourceContext, 'TouchpointMedical.')" }
              }
            ],
            "WriteTo": [
              {
                "Name": "File",
                "Args": {
                  "path": "Logs/touchpointmedical-.json",
                  "rollingInterval": "Day",
                  "rollOnFileSizeLimit": true,
                  "fileSizeLimitBytes": 10485760,
                  "retainedFileCountLimit": 7,
                  "formatter": "Serilog.Formatting.Json.JsonFormatter, Serilog"
                }
              }
            ]
          }
        }
      }
    ]
  }
}
